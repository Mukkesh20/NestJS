{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "monorepo": true,
  "root": "",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "tsconfig.app.json"
  },
  "projects": {
    "task-management": {
      "type": "application",
      "root": "",
      "entryFile": "main",
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.app.json"
      }
    },
    "server": {
      "type": "application",
      "root": "server",
      "entryFile": "main",
      "sourceRoot": "server/src",
      "compilerOptions": {
        "tsConfigPath": "server/tsconfig.app.json"
      }
    }
  }
}